{
  "name": "withAr5iv",
  "short_name": "withAr5iv",
  "version": "1.0",
  "manifest_version": 3,
  "description": "ArXiv with Ar5iv",
  "host_permissions": [
      "https://arxiv.org/",
      "http://arxiv.org/"
  ],
  "content_scripts": [{
    "matches": [ "*://*.arxiv.org/*" ],
    "js": ["withAr5iv.js"],
    "all_frames": true
  }],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  }

}

